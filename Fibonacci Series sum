class HelloWorld {
    public static void main(String[] args) {
        int a = 0,b = 1;
        int n = 4;
        int c = a+b;
        int sum = 0;
        System.out.print("Fibonacci series : "+a+" "+b);
        for(int i = 3;i<= 2*n+1;i++){
            System.out.print(" "+c);
            if(i%2==1){
                sum+=c;
            }
            a = b;
            b = c;
            c = a+b;
        }
        System.out.println();
        System.out.print("Sum : "+sum);
    }
}
